# Variables de entorno para el proyecto PWA React TypeScript

# Google Maps Configuration
VITE_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
VITE_GOOGLE_MAPS_MAP_ID=your_google_maps_map_id_here

# Intervalos de verificación PWA (configuración original)
VITE_PWA_UPDATE_INTERVAL=300000
VITE_PWA_INSTALL_PROMPT_TIMEOUT=5000
VITE_PWA_INITIAL_CHECK_DELAY=3000
VITE_PWA_MIN_FORCED_CHECK_INTERVAL=30000

# Configuración de desarrollo de PWA
VITE_PWA_SHOW_DEBUG_BUTTONS=true
VITE_PWA_VERBOSE_LOGGING=true
VITE_PWA_ENABLE_FORCE_INSTALL=true 